{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"json-server db/db.json --routes db/routes.json --delay 2000\" \"ng serve\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "check": "npm run check:style && npm run check:code",
    "check:style": "prettier --check .",
    "check:code": "eslint .",
    "fix": "npm run fix:style && npm run fix:code",
    "fix:code": "prettier --write .",
    "fix:style": "eslint --fix ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.2.0",
    "@angular/cdk": "13.2.3",
    "@angular/common": "13.2.0",
    "@angular/compiler": "13.2.0",
    "@angular/core": "13.2.0",
    "@angular/flex-layout": "13.0.0-beta.38",
    "@angular/forms": "13.2.0",
    "@angular/material": "13.2.3",
    "@angular/platform-browser": "13.2.0",
    "@angular/platform-browser-dynamic": "13.2.0",
    "@angular/router": "13.2.0",
    "flag-icons": "6.1.1",
    "lodash": "4.17.21",
    "rxjs": "7.5.4",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.2.3",
    "@angular/cli": "13.2.3",
    "@angular/compiler-cli": "13.2.0",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "concurrently": "7.0.0",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.3.0",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.1",
    "json-server": "0.17.0",
    "prettier": "2.5.1",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  }
}
